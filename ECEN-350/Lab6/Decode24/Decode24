TD_Decode24Test_v.passTest ;
    %load/vec4 v0x55a30baad0_0;
    %load/vec4 v0x55a30babb0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 33 "$display", "%s passed", v0x55a30bad50_0 {0 0 0};
    %load/vec4 v0x55a30bac90_0;
    %addi 1, 0, 8;
    %store/vec4 v0x55a30bac90_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 34 "$display", "%s failed: %d should be %d", v0x55a30bad50_0, v0x55a30baad0_0, v0x55a30babb0_0 {0 0 0};
T_1.3 ;
    %end;
