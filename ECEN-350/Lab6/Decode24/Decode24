v0x55a30ba800_0 .var "passed", 7 0;
TD_Decode24Test_v.allPassed ;
    %load/vec4 v0x55a30ba800_0;
    %load/vec4 v0x55a3091b30_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 41 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 42 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
